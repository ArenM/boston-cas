= form.input :first_name
= form.input :middle_name
= form.input :last_name
= form.input :date_of_birth, as: :date_picker
= form.input :ssn, label: "SSN", input_html: {maxlength: 9}
- if can_edit_all_clients?
  .jAgency
    = form.association :agency, input_html: {class: :select2}
= form.input :contact_id, collection: @contacts, as: :grouped_select, group_method: :last, selected: @contact_id, include_blank: 'None', label: "Default #{_('Shelter Agency')} Contact", input_html: {class: 'select2 jContact' }
- if can_add_cohorts_to_deidentified_clients? && Warehouse::Base.enabled?
  = form.input :active_cohort_ids, collection: Warehouse::Cohort.active, input_html: { multiple: true, class: 'select2' }, label: "Cohorts"
= form.input :limited_release_on_file, label: 'Limited Coordinated Access System Release on file'
= form.input :full_release_on_file, label: 'Full Housing Access Network Release on file'
= form.input :active_client
= form.input :eligible_for_matching